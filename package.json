{
  "name": "directuspt",
  "version": "0.0.4",
  "description": "Directus CLI Tools to help with snapshots and seeding operations",
  "author": "Condense <info@condense.tech>",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": "./bin/run",
  "files": [
    "/bin",
    "/dist/**/*.js"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "npm run build && bin/run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "build": "npm run clean && esbuild ./src/index.ts ./src/cli.ts --bundle --platform=node --target=node14 --outdir=dist/",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@directus/types": "^10.1.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "esbuild": "^0.18.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "<5.1.0"
  },
  "dependencies": {
    "@directus/sdk": "^11.0.1",
    "axios": "^1.4.0",
    "commander": "^11.0.0",
    "lodash-es": "^4.17.21"
  }
}
